{
    "name": "functions",
    "version": "1.0.0",
    "description": "Firebase Functions for Telegram Ads Marketplace",
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "serve": "npm run build && firebase emulators:start --only functions",
        "shell": "npm run build && firebase functions:shell",
        "start": "npm run shell",
        "deploy": "firebase deploy --only functions",
        "logs": "firebase functions:log",
        "test": "jest"
    },
    "engines": {
        "node": "20"
    },
    "dependencies": {
        "@cryptography/aes": "^0.1.1",
        "buffer": "^6.0.3",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "firebase-admin": "^11.11.0",
        "firebase-functions": "^4.5.0",
        "telegraf": "^4.15.3",
        "telegram": "^2.26.22",
        "tonweb": "^0.0.66",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.15",
        "@types/express": "^4.17.21",
        "@types/node": "^18.18.0",
        "@types/uuid": "^9.0.6",
        "typescript": "^5.2.2"
    }
}
